#!/usr/bin/env bash
set -euo pipefail

FILE_TYPE="Test File Type Fail"
FILE_EXTENSION="extf"

case "$1" in
  discover_keychains) ;;
  register_with_keychain)
    echo "${FILE_TYPE}"
    if [[ -v PW_TEST_PLUGIN_FAIL ]]; then
      echo yes
    else
      echo no
    fi
  ;;
  register_with_extension)
    echo "${FILE_TYPE}"
    echo "${FILE_EXTENSION}"
    if [[ -v PW_TEST_PLUGIN_FAIL ]]; then
      echo yes
    else
      echo no
    fi
  ;;
esac
